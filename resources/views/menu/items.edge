@each(c in categories)
<style>
a.{{convertCatName(c.desc)}}:hover::after {
    display: table-cell;
    width: 100px;  
    border-bottom: 5px solid {{c.color}};
    padding-bottom: 5px;
    content: '';
}
.button.category-divider-{{convertCatName(c.desc)}} {
  background: {{c.color}};
}
.category-divider-{{convertCatName(c.desc)}} {
  border: 3px solid {{c.color}};
}</style>
