  for (var i = 0; i < cart.length; i++) {
    t += cart[i].quantity
  }
  if (t < 5) {
    var d = 5 - t
    toastr["error"]("Please add at least "+d+" more items to cart!")
  } else {
    if(window.location.href.includes('checkout')) {
      $('html').removeClass('nav-open')
    } else {
      window.location.replace('/checkout')
    }
  }
})




